{
  "name": "mot-lang-support",
  "displayName": "Motive Language Support",
  "description": "Language highlighting extension for Motive (.mot) files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/nocpiun/mot-lang-support"
  },
  "contributes": {
    "languages": [{
      "id": "mot",
      "aliases": ["Motive", "mot"],
      "extensions": [".mot"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mot",
      "scopeName": "source.mot",
      "path": "./syntaxes/mot.tmLanguage.json"
    }]
  }
}
